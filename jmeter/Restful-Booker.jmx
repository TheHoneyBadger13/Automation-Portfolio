            <stringProp name="script">import groovy.json.JsonSlurper
import groovy.json.JsonOutput

def response = new JsonSlurper().parseText(prev.getResponseDataAsString())

// Handle simple fields
vars.put(&quot;firstname&quot;, response.booking.firstname.toString())
vars.put(&quot;lastname&quot;, response.booking.lastname.toString())
vars.put(&quot;totalprice&quot;, response.booking.totalprice.toString())
vars.put(&quot;depositpaid&quot;, response.booking.depositpaid.toString())
vars.put(&quot;additionalneeds&quot;, response.booking.additionalneeds.toString())

// Handle the nested object correctly using JsonOutput
def bookingDatesJson = JsonOutput.toJson(response.booking.bookingdates)
vars.put(&quot;bookingdates&quot;, bookingDatesJson)

// Handle the top-level ID
vars.put(&quot;bookingid&quot;, response.bookingid.toString())</stringProp>
